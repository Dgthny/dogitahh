--rotasi growx

seedid,fgorbg,blockordif = 5667,"fg","no"   --write seed id , like if lgrid seed write 5667 and if item block write fg if item background write bg

worlds = {"scqwt","bhgbt",("xpcfc","gtweu"}   --rotasi world, kalo mau add tinggal tambah ,"nameworld"

JoinWorldWithID,worldsdoorid = "false","x" --ganti jadi true kalo mau masuk pake id

worldwhile = "false" --loop world, ubah jadi true kalo mau loop world dari awal

joinstop,autoban = "true","true"

itemtrash = {5040,5042,5044,5032,5034,5036,5038,5024,5026,5028,5030}

itemname ="world_lock_10_pack"     --nama pack contoh ssp_10_pack, ubah jadi gems_pack kalo mau save gems

packgems = 20000              --harga packnya

packitemid = {242}  --item id dari packnya misal WL itu 242

dropitems = "false" 

amount = 200    ---200 or 4000
owner ="dgstr" --akun owner/yg mau dipake buat control
droppedworld,droppedworldid = "dgtbabu1","dgt57" 

seeddropworld,seeddropworldid = "dgtbabu1","dgt99"

botname,botpassword = "dgtrdp3","janda123@"       --botname pasword

--------------WEBHOOK-------------------
tittlr = "AUTO ROTATION" --Tittle webhook
webhook = "https://discord.com/api/webhooks/1007950007485354024/vqVkv_Qngm1qmCU-GltDJm6GVYxckgjmEPbKS9VoZErAx3LOxH7-5F-QD4kl5ECXQkCB"
 --webhook url
YourDiscordid= "991204570380382218" --id diskord
