              sleep(2000)
              worldegirici(worlds[i]:lower())
              findPath(pozisyonx, pozisyony)
              sleep(1000)
            end
          end
        end
        break_tile(pozisyonx,pozisyony)
        coptrashs()
      end
if blockordif == "no" then
        if fgorbg == "fg" then
            custom_coordinate_pnb = true
coordinate_x = 3
coordinate_y = 2

 local function gopos()

	if custom_coordinate_pnb then

		if math.floor((getBot()).x / 32) ~= coordinate_x - 1 or math.floor((getBot()).y / 32) ~= coordinate_y - 1 then
			findPath(coordinate_x - 1, coordinate_y - 1)
			sleep(1000)
		end
	end
end
          while getTile(math.floor(getBot().x /32)-2,math.floor(getBot().y /32)).fg == 0  and findItem(blockid) > 0 do
            place(blockid,-2,0)
            sleep(delayplace)
          end
