        end
    end
end

function SaveTheDirtSeeds()
    if SaveDirt then 
        local CurrentWorld = getBot().world 
        if findItem(3) > 150 then 
            sleep(1500)
            sendPacket(3,"action|join_request\nname|".. Save_World)
            sleep(5000)
            sendPacket(3,"action|join_request\nname|".. Save_World .. "|" .. SaveWorld_Door_ID)
            sleep(4500)
        end
        for _, tile in pairs(getTiles()) do 
            if tile.fg == BGForDirt or tile.bg == BGForDirt then 
                sleep(500)
                findPath(tile.x, tile.y)
                sleep(800)
            end
        end
        while findItem(3) > 100 do 
            drop(3)
            sleep(1500)
            move(-1,0)
            sleep(500)
        end
        if findItem(3) < 150 then
            GonWebhook("<:ExcellentGT:970324153003552769> | Bot : "..getBot().name.."\n<:mega:984686541383290940> | Information : Done Dropping Dirt Seeds... Total Dirt Seeds Dropped ".. SeedsScanner(3) .."\n<:green_circle:984666691927691274> | Status : Online\n<:globe_with_meridians:984683698123337828> | Current World : "..getBot().world.."")
            sleep(1500)
            sendPacket(3,"action|join_request\nname|".. CurrentWorld)
            sleep(5000)
        end
        move(2,0)
        sleep(500)
        move(1,0)
        sleep(500)
    end
end

function SaveTheCaveBackgroundSeeds()
    if SaveCaveBackground then 
        local CurrentWorld = getBot().world 
        if findItem(15) > 150 then 
            sleep(1500)
            sendPacket(3,"action|join_request\nname|".. Save_World)
            sleep(5000)
            sendPacket(3,"action|join_request\nname|".. Save_World .. "|" .. SaveWorld_Door_ID)
            sleep(4500)
        end
        for _, tile in pairs(getTiles()) do 
            if tile.fg == BGForCaveBackground or tile.bg == BGForCaveBackground then 
                sleep(500)
                findPath(tile.x, tile.y)
                sleep(800)
            end
        end
        while findItem(15) > 100 do 
            drop(15)
            sleep(1500)
            move(-1,0)
            sleep(500)
        end
        if findItem(15) < 150 then
            GonWebhook("<:ExcellentGT:970324153003552769> | Bot : "..getBot().name.."\n<:mega:984686541383290940> | Information : Done Dropping Cave Background Seeds... Total Cave Background Seeds Dropped ".. SeedsScanner(15) .."\n<:green_circle:984666691927691274> | Status : Online\n<:globe_with_meridians:984683698123337828> | Current World : "..getBot().world.."")
            sleep(1500)
            sendPacket(3,"action|join_request\nname|".. CurrentWorld)
            sleep(5000)
        end
        move(2,0)
        sleep(500)
        move(1,0)
        sleep(500)
    end
end

