	},
	Other_setting = {
		repeat_world = false, -- when all farm finished, if you set false: removing bot and if you set true: start again to first farm world
		url_webhook = "https://discord.com/api/webhooks/1007950222867046522/tAgAsOZUHRrRXTSgLTZO2uIft-zqsbPTjHTG_iXR-unz1sSAmokrWvZf6W2P2X7Z3LCz",
