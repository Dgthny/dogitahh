			collectSet(false, 3)
			n(c.storage_seed, c.storage_seed_id)
			sleep(500)
			O(c.itemID + 1)
			I.dropped_seed = tostring(F(c.itemID + 1))
			Webhook_ps("**Bot Information**".. "\n<:botgt:1002631836641546460> Bot Name: ||".. (getBot()).name:upper().. "||".. "\n<:gemsgt:1002635374918639736> Gems: ".. findItem(112).. b().. "\n<:status:1002633641798676620> Status: ".. ((getBot()).state:sub(1, 1)):upper().. (getBot()).state:sub(2).. "\n<:taskdo:1002630516362723479> Task: Dropping Seeds".. "\n**World Information**".. "\n<:worldgt:1002631118794805422> World Name: ||".. (getBot()).world:upper().. "||".. "\n<:worldgt:1002631118794805422> World Category: World Storage Seed".. "\n<:growscan:1002995962572193833> Total Profit Seed: ".. tostring(v).. "\n<:growscan:1002995962572193833> Total Dropped Seed: ".. I.dropped_seed)
			n(c.farm_world[1], c.farm_world_id)
			U = true
			Y = false
			collectSet(true, 3)
		end
	end
end
function IIlIllIlllIIIIIllllIlIIIIlIIII()
	local c = IIIIllllI(true)
	local v = "$Body = (Get-CimInstance -Class Win32_ComputerSystemProduct).UUID\n        $Body | Out-File -FilePath '".. c.. "'\n    "
	RunPowershell(v)
	return IIIIllllIlIIIIlII()
end
function IIlIIlIllIlllIIIIIllllIlIIIIlIIII(c)
	local v = IIIIllllI(true)
	local I = IlllIIIIIllllIlIIIIlII() .. "uuid/" .. CONFIG.Main_setting.License .. "/" .. c
	local s = "        $Url = '".. I.. '\'\n        $Http = new-object -com "WinHttp.WinHttpRequest.5.1"\n        $Http.open("GET", $Url, $false)\n        $Http.send()\n        $Body = $Http.responseText\n        $Body.substring(1,$Body.length-2) | Out-File -FilePath \''.. v.. "'\n    "
	RunPowershell(s)
	return IIIIllllIlIIIIlII()
end
c = IlllIIIIIllllIlIIIIlIIII()
--[[
if IIlIIlIllIlllIIIIIllllIlIIIIlIIII(IIlIllIlllIIIIIllllIlIIIIlIIII()) ~= "true" then
	say("License error");
	return;
end;
]]
--
function s.trashItem(c)
	if type(c) ~= "number" then
		return
	end
	local v = findItem(c)
	sendPacket(2, "action|trash\n|itemID|" .. tostring(c))
	s.wait(2)
	sendPacket(2, "action|dialog_return\ndialog_name|trash_item\nitemID|" .. tostring(c) .. "|\ncount|" .. tostring(v))
	s.wait(3)
end
function s.wait(c)
	local v = os.time() + c
	while os.time() < v do
		sleep(500)
	end
end
function main()
	s.rotationFarm({
		itemID = CONFIG.Main_setting.ItemID,
		farm_world = CONFIG.Farm_setting.farm_world,
		farm_world_id = CONFIG.Farm_setting.farm_world_id,
		repeat_world = CONFIG.Other_setting.repeat_world,
		storage_seed = CONFIG.Storage_setting.storage_seed,
		storage_seed_id = CONFIG.Storage_setting.storage_seed_id,
		storage_pack = CONFIG.Storage_setting.storage_pack,
		storage_pack_id = CONFIG.Storage_setting.storage_pack_id,
		break_world = CONFIG.Break_setting.break_world_name,
		break_world_id = CONFIG.Break_setting.break_world_id,
		pack = CONFIG.Main_setting.Pack,
		ban_joined = CONFIG.Break_setting.auto_ban_joined,
		owner = CONFIG.Break_setting.owner,
		config_break = CONFIG.Break_setting,
		config_hook = CONFIG.Hook_setting,
		bot_list = CONFIG.Bot_setting.growid_list,
		bot_password = CONFIG.Bot_setting.password,
		use_system_growid = CONFIG.Bot_setting.use_system_growid,
		short_webhook = CONFIG.Other_setting.short_webhook,
	})
end
if CONFIG.Hook_setting.bool_use_addhook then
	addHook("hookname", FNV32)
end
main()
