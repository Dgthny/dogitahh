    for _, tile in pairs(getTiles()) do
        while tile.fg == 4 do
            Reconnect()
            findPath(tile.x,tile.y-1)
            punch(0,1)
            sleep(190)
            if getTile(tile.x,tile.y).fg == 0 then
                collect(2)
                break
            end
            if findItem(5) > 150 then
                GonWebhook("<:ExcellentGT:970324153003552769> | Bot : "..getBot().name.."\n<:mega:984686541383290940> | Information : Lava Seeds Is More Than 150... Saving Lava Seeds In ".. Save_World .. "\n<:green_circle:984666691927691274> | Status : Online\n<:globe_with_meridians:984683698123337828> | Current World : "..getBot().world.."")
