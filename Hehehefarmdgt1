function GetDirtSeeds()
    local CurrentWorld = getBot().world
    if findItem(3) == 0 then 
        sleep(1500)
        sendPacket(3,"action|join_request\nname|".. Save_World)
        sleep(5000)
        sendPacket(3,"action|join_request\nname|".. Save_World .. "|" .. SaveWorld_Door_ID)
        sleep(4500)
        for _, floating in pairs(getObjects()) do 
            if floating.id == 3 then 
                findPath((floating.x+10) / 32, (floating.y+10) / 32)
                sleep(800)
                collect(2)
                sleep(1500)
            end
        end
        if findItem(3) > 1 then 
            GonWebhook("<:ExcellentGT:970324153003552769> | Bot : "..getBot().name.."\n<:mega:984686541383290940> | Information : Dirt Seeds Is More Than 1... Going Back On Creating Dirt Farms For You My Master.\n<:green_circle:984666691927691274> | Status : Online\n<:globe_with_meridians:984683698123337828> | Current World : "..getBot().world.."")
            sleep(1500)
            sendPacket(3,"action|join_request\nname|".. CurrentWorld)
            sleep(5000)
        end
        move(2,0)
        sleep(500)
        move(1,0)
        sleep(500)
    end
end

function PlantAndHarvestDirt()
    while findItem(2) == 0 do
        for x = 2, 22  do
            if findItem(2) == 200 then 
                break
            end
            if findItem(3) == 0 then
                GonWebhook("<:ExcellentGT:970324153003552769> | Bot : "..getBot().name.."\n<:mega:984686541383290940> | Information : Dirt Seeds Is 0... Taking Dirt Seeds In ".. Save_World .. "\n<:green_circle:984666691927691274> | Status : Online\n<:globe_with_meridians:984683698123337828> | Current World : "..getBot().world.."")
                GetDirtSeeds() 
                sleep(250)
                GonWebhook("<:ExcellentGT:970324153003552769> | Bot : "..getBot().name.."\n<:mega:984686541383290940> | Information : "..getBot().name.." Is Going To Place Dirts To Make A Layer In "..getBot().world.."\n<:green_circle:984666691927691274> | Status : Online\n<:globe_with_meridians:984683698123337828> | Current World : "..getBot().world.."")
            end
            while getTile(x, 23).ready do
                findPath(x, 23)
                punch(0, 0)
                sleep(155)
                collect(2)
            end
            while getTile(x, 23).fg == 0 and findItem(3) > 0 do
                findPath(x, 23)
                place(3, 0, 0)
                sleep(90)
            end
            Reconnect()
        end
    end
end

function PlaceTheDirts()
    GonWebhook("<:ExcellentGT:970324153003552769> | Bot : "..getBot().name.."\n<:mega:984686541383290940> | Information : "..getBot().name.." Is Going To Place Dirts To Make A Layer In "..getBot().world.."\n<:green_circle:984666691927691274> | Status : Online\n<:globe_with_meridians:984683698123337828> | Current World : "..getBot().world.."")
    for y = 52, 2, -2 do
        for x = 2, 97 do
            if getTile(x, y).fg == 0 then
                PlantAndHarvestDirt()
                findPath(x, y - 1)
                while getTile(x, y).fg == 0 and findItem(2) > 0 do
                    place(2, 0, 1)
                    sleep(190)
                end
                Reconnect()
            end
        end
    end
end

function DirtFarmDone()
    for x = 2, 22  do
        while getTile(x,23).fg == 3 do
            findPath(x,23)
            punch(0,0)
            sleep(190)
            TrashTheJunks()
            if getTile(x,23).fg == 0 then
                collect(2)
                sleep(40)
            end 
            Reconnect()
        end
    end
end

function BreakTheSignalJammer()
    for _,t in pairs(getTiles()) do
        if t.fg == 226 then
          findPath(t.x, t.y-1)
          sleep(250)
          while getTile(t.x, t.y).fg ~= 0 or getTile(t.x, t.y).bg ~= 0 do
            punch(0,1)
            sleep(125)
          end
        end
    end
end

function NeedsToGenerateAWorld()
    if GenerateRandomWorld == 1 then
        GonWebhook("<:ExcellentGT:970324153003552769> | Bot : "..getBot().name.."\n<:mega:984686541383290940> | Information : Finding "..WorldLetters.." Letter World Please Wait.\n<:green_circle:984666691927691274> | Status : Online\n<:globe_with_meridians:984683698123337828> | Current World : "..getBot().world.."") 
        GenerateAWorld()
        sleep(300)
        PutWlAndJammer()
    end

    if GenerateRandomWorld == 1 then
        LogTheName("World : "..getBot().world) 
        sleep(300)
        GonWebhook("<:ExcellentGT:970324153003552769> | Bot : "..getBot().name.."\n<:mega:984686541383290940> | Information : "..WorldLetters.." Letter World Found And The Name Of It Was "..getBot().world.."\n<:green_circle:984666691927691274> | Status : Online\n<:globe_with_meridians:984683698123337828> | Current World : "..getBot().world.."")
        sleep(500)
    end
end

