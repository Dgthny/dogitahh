	Bot_setting = {
		growid_list = {
			"Bot1",
			"Bot2",
			"Bot3",
		},
		password = "password", -- every password of growid_list must be same
		use_system_growid = false, -- if true when world-farm finished, bot will change to next bot in growid_list
	},
	Main_setting = {
		License = "license in payy server", -- lock hwid, 1 license 3 device. For more info join discord payy
		ItemID = 5666,
		Pack = "world_lock_10_pack", -- you can check more about pack in discord payy not all pack support
	},
	Farm_setting = {
		farm_world = {
			"spxae","tfhgdch",
		},
		farm_world_id = "xxx",
	},
	Storage_setting = {
		storage_seed = "xxx",
		storage_seed_id = "xxx",
		storage_pack = "xxx",
		storage_pack_id = "xxx",
	},
	Break_setting = {
		bool_break_other_world = false	,
		break_world_name = "xxx",
		break_world_id = "xxx",
		-- you can set tile into 1,2,3,4 or 5 tile when breaking block
		tile = 1,
		delay_break = 100, -- millisecond
		delay_put = 100, -- millisecond
		auto_ban_joined = true, -- you need access world lock if wanna auto-ban-joined
		owner = "xxx", -- when autobanjoined true, whitelist this growid
	},
	Hook_setting = {
		-- if you set bool_use_addhook false, in webhook discord you can see level of bot
		bool_use_addhook = true,
