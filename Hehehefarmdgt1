              sleep(2000)
              worldegirici(worlds[i]:lower())
              findPath(pozisyonx, pozisyony)
              sleep(1000)
            end
          end
        end
        if fgorbg == "bg" then

          if getTile(math.floor(getBot().x /32) -1,math.floor(getBot().y /32)).bg == 0 then
            place(blockid,-1,0)
            sleep(delayplace)
            if joinstop =="true" then
              if #getPlayers() > 0 then
                while girischeck() do
                  girischeck()
