function TakeAndPlacePlatform()
    GonWebhook("<:ExcellentGT:970324153003552769> | Bot : "..getBot().name.."\n<:mega:984686541383290940> | Information : "..getBot().name.." Is Going To Take Platform In "..PlatformWorld.." Then Placing It In "..getBot().world.."\n<:green_circle:984666691927691274> | Status : Online\n<:globe_with_meridians:984683698123337828> | Current World : "..getBot().world.."")
    local CurrentWorld = getBot().world
    if findItem(Platform_ID) < 52 then
        sendPacket(3,"action|join_request\nname|" ..PlatformWorld)
        sleep(5000)
        sendPacket(3,"action|join_request\nname|" ..PlatformWorld.. "|" ..PlatformWorldID)
        sleep(4500)
        collect(2)
        sleep(4500)
        if findItem(Platform_ID) > 104 then
            move(-1,0)
            DropTheItems(Platform_ID,findItem(Platform_ID) - 104)
        end
        sendPacket(3,"action|join_request\nname|" ..CurrentWorld)
        sleep(5000)
        move(2,0)
        sleep(500)
        move(1,0)
        sleep(200)
    end
    for i = 2, 52, 2 do
        while getTile(1,i).fg == 0 do
            if findItem(Platform_ID) == 0 then
                break
            end
            findPath(1,i-1)
            place(Platform_ID,0,1)
            sleep(190)
        end
    end
    for i = 2, 52, 2 do
        while getTile(98,i).fg == 0 do
            if findItem(Platform_ID) == 0 then
                break
            end
            findPath(98,i-1)
            place(Platform_ID,0,1)
            sleep(190)
        end
    end
end

