							sleep(l.delay_break)
							if I then
								z = (getTile(math.floor(getBot().x() / 32) - 2, math.floor(getBot().y() / 32) + G)).fg
								F = (getTile(math.floor(getBot().x() / 32) - 1, math.floor(getBot().y() / 32) + G)).fg
							else
								z = (getTile(math.floor(getBot().x() / 32) - 2, math.floor(getBot().y() / 32) + G)).bg
								F = (getTile(math.floor(getBot().x() / 32) - 1, math.floor(getBot().y() / 32) + G)).bg
							end
						end
					end
				end
				if z == 0 or F == 0 then
					if z == 0 then
						place(v, -2, G)
						sleep(l.delay_put)
					end
					if F == 0 then
						place(v, -1, G)
						sleep(l.delay_put)
					end
					for c, v in pairs(K) do
						if math.floor(findItem(v)) >= 180 then
							s.trashItem(v)
						end
					end
					if math.floor(findItem(c.itemID + 1)) >= 190 then
						return
					end
				end
			elseif l.tile >= 3 then
				if z ~= 0 and z ~= nil or F ~= 0 and F ~= nil or m ~= 0 and m ~= nil or p ~= 0 and p ~= nil or w ~= 0 and w ~= nil then
					for c = 1, l.tile, 1 do
						if c == 1 and z ~= nil then
							i = z
						elseif c == 2 and F ~= nil then
							i = F
						elseif c == 3 and m ~= nil then
							i = m
						elseif c == 4 and p ~= nil then
							i = p
						elseif c == 5 and w ~= nil then
							i = w
						end
						if i ~= 0 then
							if (getTile(math.floor(getBot().x() / 32), math.floor(getBot().y() / 32))).fg == 6 then
								sendPacket(3, "action|join_request\nname|" .. D .. "|" .. X)
								s.wait(10)
								while true do
									if (getTile(math.floor(getBot().x() / 32), math.floor(getBot().y() / 32))).fg == 6 then
										sendPacket(3, "action|join_request\nname|" .. D .. "|" .. X)
										s.wait(10)
									elseif
										(getTile(math.floor(getBot().x() / 32), math.floor(getBot().y() / 32))).fg ~= 6
										or string.lower((getBot()).state) ~= 5
									then
										break
									end
								end
								findPath(J[1], J[2])
								sleep(500)
							end
							place(18, T[c], G)
							sleep(l.delay_break)
							if I then
								if l.tile == 3 then
									z = (getTile(math.floor(getBot().x() / 32) - 2, math.floor(getBot().y() / 32) + G)).fg
									F = (getTile(math.floor(getBot().x() / 32) - 1, math.floor(getBot().y() / 32) + G)).fg
									m = (getTile(math.floor(getBot().x() / 32) + 1, math.floor(getBot().y() / 32) + G)).fg
								elseif l.tile == 4 then
									z = (getTile(math.floor(getBot().x() / 32) - 2, math.floor(getBot().y() / 32) + G)).fg
									F = (getTile(math.floor(getBot().x() / 32) - 1, math.floor(getBot().y() / 32) + G)).fg
									m = (getTile(math.floor(getBot().x() / 32) + 1, math.floor(getBot().y() / 32) + G)).fg
									p = (getTile(math.floor(getBot().x() / 32) + 2, math.floor(getBot().y() / 32) + G)).fg
								elseif l.tile == 5 then
									z = (getTile(math.floor(getBot().x() / 32) - 2, math.floor(getBot().y() / 32) + G)).fg
									F = (getTile(math.floor(getBot().x() / 32) - 1, math.floor(getBot().y() / 32) + G)).fg
									m = (getTile(math.floor(getBot().x() / 32) + 0, math.floor(getBot().y() / 32) + G)).fg
									p = (getTile(math.floor(getBot().x() / 32) + 1, math.floor(getBot().y() / 32) + G)).fg
									w = (getTile(math.floor(getBot().x() / 32) + 2, math.floor(getBot().y() / 32) + G)).fg
								end
							elseif l.tile == 3 then
								z = (getTile(math.floor(getBot().x() / 32) - 2, math.floor(getBot().y() / 32) + G)).bg
								F = (getTile(math.floor(getBot().x() / 32) - 1, math.floor(getBot().y() / 32) + G)).bg
								m = (getTile(math.floor(getBot().x() / 32) + 1, math.floor(getBot().y() / 32) + G)).bg
							elseif l.tile == 4 then
								z = (getTile(math.floor(getBot().x() / 32) - 2, math.floor(getBot().y() / 32) + G)).bg
								F = (getTile(math.floor(getBot().x() / 32) - 1, math.floor(getBot().y() / 32) + G)).bg
								m = (getTile(math.floor(getBot().x() / 32) + 1, math.floor(getBot().y() / 32) + G)).bg
								p = (getTile(math.floor(getBot().x() / 32) + 2, math.floor(getBot().y() / 32) + G)).bg
							elseif l.tile == 5 then
								z = (getTile(math.floor(getBot().x() / 32) - 2, math.floor(getBot().y() / 32) + G)).bg
								F = (getTile(math.floor(getBot().x() / 32) - 1, math.floor(getBot().y() / 32) + G)).bg
								m = (getTile(math.floor(getBot().x() / 32) + 0, math.floor(getBot().y() / 32) + G)).bg
								p = (getTile(math.floor(getBot().x() / 32) + 1, math.floor(getBot().y() / 32) + G)).bg
								w = (getTile(math.floor(getBot().x() / 32) + 2, math.floor(getBot().y() / 32) + G)).bg
							end
						end
					end
				end
				if l.tile == 3 then
					if z == 0 and (F == 0 and m == 0) then
						for c = 1, 3, 1 do
							place(v, T[c], G)
							sleep(l.delay_put)
						end
						for c, v in pairs(K) do
							if math.floor(findItem(v)) >= 180 then
								s.trashItem(v)
							end
						end
						if math.floor(findItem(c.itemID + 1)) >= 190 then
							return
						end
					end
				elseif l.tile == 4 then
					if z == 0 and (F == 0 and (m == 0 and p == 0)) then
						for c = 1, 4, 1 do
							place(v, T[c], G)
							sleep(l.delay_put)
						end
						for c, v in pairs(K) do
							if math.floor(findItem(v)) >= 180 then
								s.trashItem(v)
							end
						end
						if math.floor(findItem(c.itemID + 1)) >= 190 then
							return
						end
					end
				elseif l.tile == 5 then
					if z == 0 and (F == 0 and (m == 0 and (p == 0 and w == 0))) then
						for c = 1, 5, 1 do
							place(v, T[c], G)
							sleep(l.delay_put)
						end
						for c, v in pairs(K) do
							if math.floor(findItem(v)) >= 180 then
								s.trashItem(v)
							end
						end
						if math.floor(findItem(c.itemID + 1)) >= 190 then
							return
						end
					end
				end
			end
			if g then
				if #getPlayers() > 1 then
					for c, v in pairs(getPlayers()) do
						if v.name:lower() ~= C:lower() and v.name:lower() ~= (getBot()).name:lower() then
							say("/ban " .. v.name:lower())
							s.wait(1)
						end
					end
				end
			end
			if (getBot()).state ~= 5 then
				Webhook_ps("**Bot Information**".. "\n<:botgt:1002631836641546460> Bot Name: ||".. (getBot()).name:upper().. "||".. "\n<:gemsgt:1002635374918639736> Gems: ".. findItem(112).. b().. "\n<:status:1002633641798676620> Status: Disconnect".. "\n<:taskdo:1002630516362723479> Task: Breaking Blocks".. "\n**World Information**".. "\n<:worldgt:1002631118794805422> World Name: ||".. (getBot()).world:upper().. "||".. "\n<:worldgt:1002631118794805422> World Category: World Break".. N().. h().. W().. P())
				while (getBot()).state ~= 5 do
					s.wait(5)
				end
				Webhook_ps("**Bot Information**".. "\n<:botgt:1002631836641546460> Bot Name: ||".. (getBot()).name:upper().. "||".. "\n<:gemsgt:1002635374918639736> Gems: ".. findItem(112).. b().. "\n<:status:1002633641798676620> Status: Reconnect".. "\n<:taskdo:1002630516362723479> Task: Breaking Blocks".. "\n**World Information**".. "\n<:worldgt:1002631118794805422> World Name: ||".. (getBot()).world:upper().. "||".. "\n<:worldgt:1002631118794805422> World Category: World Break".. N().. h().. W().. P())
				n(D, X)
				sleep(500)
				findPath(J[1], J[2])
				sleep(500)
			end
		end
		z = true
		while true do
			if (getBot()).state ~= 5 then
				break
			end
			if (getTile(math.floor(getBot().x() / 32 - 2), math.floor(getBot().y() / 32 + G))).fg == v then
				while (getTile(math.floor(getBot().x() / 32 - 2), math.floor(getBot().y() / 32 + G))).fg ~= 0 do
					punch(-2, G)
					sleep(l.delay_break)
				end
			elseif (getTile(math.floor(getBot().x() / 32 - 1), math.floor(getBot().y() / 32 + G))).fg == v then
				while (getTile(math.floor(getBot().x() / 32 - 1), math.floor(getBot().y() / 32 + G))).fg ~= 0 do
					punch(-1, G)
					sleep(l.delay_break)
				end
			elseif (getTile(math.floor(getBot().x() / 32), math.floor(getBot().y() / 32 + G))).fg == v then
				while (getTile(math.floor(getBot().x() / 32), math.floor(getBot().y() / 32 + G))).fg ~= 0 do
					punch(0, G)
					sleep(l.delay_break)
				end
			elseif (getTile(math.floor(getBot().x() / 32 + 1), math.floor(getBot().y() / 32 + G))).fg == v then
				while (getTile(math.floor(getBot().x() / 32 + 1), math.floor(getBot().y() / 32 + G))).fg ~= 0 do
					punch(1, G)
					sleep(l.delay_break)
				end
			elseif (getTile(math.floor(getBot().x() / 32 + 2), math.floor(getBot().y() / 32 + G))).fg == v then
				while (getTile(math.floor(getBot().x() / 32 + 2), math.floor(getBot().y() / 32 + G))).fg ~= 0 do
					punch(2, G)
					sleep(l.delay_break)
				end
			else
				break
			end
			sleep(100)
		end
	end
	local function i(v, I, z)
		for c, g in pairs(getTiles()) do
			if g.fg == v and math.floor(findItem(v - 1)) < 180 then
				if g.ready then
					findPath(g.x, g.y ,40)
					sleep(100)
					punch(0, 0)
					sleep(100)
					if (getTile(math.floor(getBot().x() / 32), math.floor(getBot().y() / 32))).fg == 6 then
						sendPacket(3, "action|join_request\nname|" .. I .. "|" .. z)
						s.wait(10)
						while true do
							if (getTile(math.floor(getBot().x() / 32), math.floor(getBot().y() / 32))).fg == 6 then
								sendPacket(3, "action|join_request\nname|" .. I .. "|" .. z)
								s.wait(10)
							elseif (getTile(math.floor(getBot().x() / 32), math.floor(getBot().y() / 32))).fg ~= 6 or string.lower((getBot()).state) ~= 5 then
								break
							end
						end
					end
					if (getBot()).state ~= 5 then
						Webhook_ps("**Bot Information**".. "\n<:botgt:1002631836641546460> Bot Name: ||".. (getBot()).name:upper().. "||".. "\n<:gemsgt:1002635374918639736> Gems: ".. findItem(112).. b().. "\n<:status:1002633641798676620> Status: Disconnect".. "\n<:taskdo:1002630516362723479> Task: Harvesting Trees!".. "\n**World Information**".. "\n<:worldgt:1002631118794805422> World Name: ||".. (getBot()).world:upper().. "||".. "\n<:worldgt:1002631118794805422> World Category: World Farm".. N().. h().. W().. P())
						while (getBot()).state ~= 5 do
							s.wait(2)
						end
						Webhook_ps("**Bot Information**".. "\n<:botgt:1002631836641546460> Bot Name: ||".. (getBot()).name:upper().. "||".. "\n<:gemsgt:1002635374918639736> Gems: ".. findItem(112).. b().. "\n<:status:1002633641798676620> Status: Reconnect".. "\n<:taskdo:1002630516362723479> Task: Harvesting Trees!".. "\n**World Information**".. "\n<:worldgt:1002631118794805422> World Name: ||".. (getBot()).world:upper().. "||".. "\n<:worldgt:1002631118794805422> World Category: World Farm".. N().. h().. W().. P())
						s.wait(1)
						sendPacket(3, "action|join_request\nname|" .. I .. "|" .. z)
						s.wait(5)
						while true do
							if (getTile(math.floor(getBot().x() / 32), math.floor(getBot().y() / 32))).fg == 6 then
								sendPacket(3, "action|join_request\nname|" .. I .. "|" .. z)
								s.wait(5)
							elseif (getTile(math.floor(getBot().x() / 32), math.floor(getBot().y() / 32))).fg ~= 6 or string.lower((getBot()).state) ~= 5 then
								break
							end
						end
						s.wait(2)
						findPath(g.x, g.y)
						s.wait(2)
					end
				end
			elseif math.floor(findItem(v - 1)) >= 180 then
				break
			end
		end
		if c.ban_joined then
			if #getPlayers() > 1 then
				for v, I in pairs(getPlayers()) do
					if I.name:lower() ~= c.owner:lower() and I.name:lower() ~= (getBot()).name:lower() then
						say("/ban " .. I.name:lower())
						s.wait(1)
					end
				end
			end
		end
	end
	local function e(I, z, g)
		for C, D in pairs(getTiles()) do
			if D.fg ~= 0 and (D.y > 1 and (findItem(I) > 0 and ((getTile(D.x, D.y - 1)).fg == 0 and ((getTile(D.x, D.y)).fg ~= I and ((getTile(D.x, D.y)).fg ~= I - 1 and ((getTile(D.x - 2, D.y - 1)).fg ~= 6 and (getTile(D.x + 2, D.y - 1)).fg ~= 6)))))) then
 				findPath(D.x, D.y - 1)
				sleep(100)
				place(I, 0, 0)
				sleep(100)
				if (getTile(math.floor(getBot().x() / 32), math.floor(getBot().y() / 32))).fg == 6 then
					sendPacket(3, "action|join_request\nname|" .. z .. "|" .. g)
					s.wait(10)
					while true do
						if (getTile(math.floor(getBot().x() / 32), math.floor(getBot().y() / 32))).fg == 6 then
							sendPacket(3, "action|join_request\nname|" .. z .. "|" .. g)
							s.wait(10)
						elseif
							(getTile(math.floor(getBot().x() / 32), math.floor(getBot().y() / 32))).fg ~= 6
							or string.lower((getBot()).state) ~= 5
						then
							break
						end
					end
				end
				if (getBot()).state ~= 5 then
					Webhook_ps("**Bot Information**".. "\n<:botgt:1002631836641546460> Bot Name: ||".. (getBot()).name:upper().. "||".. "\n<:gemsgt:1002635374918639736> Gems: ".. findItem(112).. b().. "\n<:status:1002633641798676620> Status: Disconnect".. "\n<:taskdo:1002630516362723479> Task: Planting Seeds".. "\n**World Information**".. "\n<:worldgt:1002631118794805422> World Name: ||".. (getBot()).world:upper().. "||".. "\n<:worldgt:1002631118794805422> World Category: World Farm".. N().. h().. W().. P())
					s.wait(1)
					while (getBot()).state ~= 5 do
						s.wait(2)
					end
					Webhook_ps("**Bot Information**".. "\n<:botgt:1002631836641546460> Bot Name: ||".. (getBot()).name:upper().. "||".. "\n<:gemsgt:1002635374918639736> Gems: ".. findItem(112).. b().. "\n<:status:1002633641798676620> Status: Reconnect".. "\n<:taskdo:1002630516362723479> Task: Planting Seeds".. "\n**World Information**".. "\n<:worldgt:1002631118794805422> World Name: ||".. (getBot()).world:upper().. "||".. "\n<:worldgt:1002631118794805422> World Category: World Farm".. N().. h().. W().. P())
					s.wait(2)
					sendPacket(3, "action|join_request\nname|" .. z .. "|" .. g)
					s.wait(10)
					while true do
						if (getTile(math.floor(getBot().x() / 32), math.floor(getBot().y() / 32))).fg == 6 then
							sendPacket(3, "action|join_request\nname|" .. z .. "|" .. g)
							s.wait(10)
						elseif
							(getTile(math.floor(getBot().x() / 32), math.floor(getBot().y() / 32))).fg ~= 6
							or string.lower((getBot()).state) ~= 5
						then
							break
						end
					end
					s.wait(2)
				end
			elseif math.floor(findItem(I)) <= 0 and v then
				if T(c.itemID + 1) < 100 then
					break
				end
				n(c.storage_seed, c.storage_seed_id)
				sleep(500)
				local z = 0
				for c, v in pairs(getObjects()) do
					if v.id == I then
						z = z + v.count
					end
				end
				if z < 100 then
					v = false
					n(c.farm_world[1], c.farm_world_id)
					break
				elseif z >= 100 then
					for v, z in pairs(getObjects()) do
						if z.id == I and z.count > 0 then
							findPath(math.floor(z.x / 32), math.floor(z.y / 32))
							sleep(500)
							collect(5)
							s.wait(2)
							if math.floor(findItem(I)) <= 0 then
								collect(5)
								s.wait(2)
							else
								n(c.farm_world[1], c.farm_world_id)
								sleep(500)
								break
							end
						end
					end
				end
			end
		end
		if c.ban_joined then
			if #getPlayers() > 1 then
				for v, I in pairs(getPlayers()) do
					if I.name:lower() ~= c.owner:lower() and I.name:lower() ~= (getBot()).name:lower() then
						say("/ban " .. I.name:lower())
						s.wait(1)
					end
				end
			end
		end
	end
	local function d(c)
		local v = {
			4584,
			340,
			5666,
			5990,
			324,
		}
		local I = {
			3004,
		}
		for v, I in pairs(v) do
			if c == I then
				return true
			end
		end
		for v, I in pairs(I) do
			if c == I then
				return false
			end
		end
	end
	local function O(c)
		local I = 0
		for v, s in pairs(getObjects()) do
			if s.id == c then
				I = I + s.count
			end
		end
		if I >= 2000 then
			v = true
		end
		while true do
			if math.floor(findItem(c)) > 0 then
				drop(c)
				s.wait(3)
				move(1, 0)
				sleep(300)
			elseif math.floor(findItem(c)) <= 0 then
				break
			end
		end
	end
