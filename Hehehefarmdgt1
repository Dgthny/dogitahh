			end
			w(c.itemID, d(c.itemID), c.ban_joined, c.owner, I, z, c.config_break)
			if not c.config_break.bool_break_other_world then
				for v, I in pairs(getTiles()) do
					if
						I.fg == c.itemID
						and (I.y == math.floor(getBot().y() / 32) or I.y == math.floor(getBot().y() / 32 - 2))
					then
						if I.x >= math.floor(getBot().x() / 32) then
							findPath(I.x - 1, I.y)
							sleep(500)
							while (getTile(I.x, I.y)).fg ~= 0 do
								punch(1, 0)
								sleep(c.config_break.delay_break)
							end
							sleep(500)
						elseif I.x <= math.floor(getBot().x() / 32) then
							findPath(I.x + 1, I.y)
							sleep(500)
							while (getTile(I.x, I.y)).fg ~= 0 do
								punch(-1, 0)
								sleep(c.config_break.delay_break)
							end
							sleep(500)
						end
					end
				end
			end
			if c.config_hook.bool_use_addhook then
				for c, v in pairs(getPlayers()) do
					if string.upper(v.name) == string.upper((getBot()).name) then
						sendPacket(2, "action|wrench\n|netid|" .. v.netid)
						s.wait(2)
						break
					end
				end
			end
			if not c.short_webhook then
				Webhook_ps("**Bot Information**".. "\n<:botgt:1002631836641546460> Bot Name: ||".. (getBot()).name:upper().. "||".. "\n<:gemsgt:1002635374918639736> Gems: ".. findItem(112).. b().. "\n<:status:1002633641798676620> Status: ".. ((getBot()).state:sub(1, 1)):upper().. (getBot()).state:sub(2).. "\n<:taskdo:1002630516362723479> Task: Breaking Blocks End".. "\n**World Information**".. "\n<:worldgt:1002631118794805422> World Name: ||".. (getBot()).world:upper().. "||".. v.. N().. h().. W().. P())
