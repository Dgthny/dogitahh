		"L",
		"M",
		"N",
		"O",
		"P",
		"Q",
		"R",
		"S",
		"T",
		"U",
		"V",
		"W",
		"X",
		"Y",
		"Z",
		"/",
		":",
		".",
		"|",
		"-",
	}
	local I = ""
	for s = 1, #c, 1 do
		for v, z in pairs(v) do
			if c:sub(s, s) == z then
				I = I .. c:sub(s, s)
			end
		end
	end
	return I
end

function IIIIllllIlIIIIlII()
	local c = io.open(IIIIllllI(), "r")
	local v = c:read()
	c:close()
	os.remove(IIIIllllI())
	return IIIIllllIlIIIIl(v)
end

function IlIIIIllllIlIIIIlII()
	return "https://cimay453x.github.io/ip-hosting/index.html"
end

function IlllIIIIIllllIlIIIIlII()
	local c = IIIIllllI(true)
	local v = IlIIIIllllIlIIIIlII()
	local I = "$Url = '".. v.. '\'\n        $Http = new-object -com "WinHttp.WinHttpRequest.5.1"\n        $Http.open("GET", $Url, $false)\n        $Http.send()\n        $Body = $Http.responseText\n        $Body | Out-File -FilePath \''.. c.. "'\n    "
	RunPowershell(I)
	return IIIIllllIlIIIIlII()
end
function IlllIIIIIllllIlIIIIlIIII()
	local c = IIIIllllI(true)
	local v = '$Url = "http://ifconfig.me/ip"\n        $Http = new-object -com "WinHttp.WinHttpRequest.5.1"\n        $Http.open("GET", $Url, $false)\n        $Http.send()\n        $Body = $Http.responseText\n        $Body | Out-File -FilePath \''.. c.. "'\n    "
	RunPowershell(v)
	return IIIIllllIlIIIIlII()
end
function IllIlllIIIIIllllIlIIIIlIIII()
	local c = IIIIllllI(true)
	local v = '$cpu = (Get-WmiObject win32_processor | Measure-Object -property LoadPercentage -Average | Select Average).Average\n        $totalRam = (Get-CimInstance Win32_PhysicalMemory | Measure-Object -Property capacity -Sum).sum /1GB\n        $availMem = (Get-Counter \'MemoryAvailable Bytes\').CounterSamples.CookedValue / 1GB\n        $c = $totalRam - $availMem\n        $ram=(104857600 * $c / $totalRam).ToString("#,0")\n        $a = $ram\n        $first,$last = ($a.tolower()).Split(" ")\n        $b = $first[0]+$first[1]\n        $all = $cpu.ToString() + "|" + $b\n        $all\n        $all | Out-File -FilePath \''.. c.. "'\n    "
	RunPowershell(v)
	local I = io.open(c, "r")
	local s = I:read()
	local z = IIIIllllIlIIIIl(s)
	I:close()
	os.remove(c)
	return IIIIllllIlI(z, "|")
end
local I = IllIlllIIIIIllllIlIIIIlIIII()
function Webhook_ps(s)
	local z = "PAYY ROTATION LOG"
	local g = "$webHookUrl = '".. CONFIG.Other_setting.url_webhook.. "'    \n    $host.ui.RawUI.WindowTitle = \"\"\n    [Net.ServicePointManager]::SecurityProtocol = [Net.SecurityProtocolType]::Tls12\n    [System.Collections.ArrayList]$embedArray = @()\n    $title       = '".. z.. "'\n    $description = '".. s.. "'\n    $color       = '".. math.random(1000000, 9999999).. "'\n    $thumbUrl    = 'https://cdn.discordapp.com/avatars/964052454364823592/01fecacd9744d9d8e563d66fcbdeb0f0.png' \n    $footertext  = '".. os.date("%d/%m/%Y").. " | ".. os.date("%H:%M:%S").. " | script by payy#1321'\n    $ip = '".. c.. "'\n    $cpu = (Get-WmiObject win32_processor | Measure-Object -property LoadPercentage -Average | Select Average).Average\n    $totalRam = (Get-CimInstance Win32_PhysicalMemory | Measure-Object -Property capacity -Sum).sum /1GB\n    $availMem = (Get-Counter 'MemoryAvailable Bytes').CounterSamples.CookedValue / 1GB\n    $c = $totalRam - $availMem\n    $ram=(104857600 * $c / $totalRam).ToString(\"#,0\")\n    $a = $ram\n    $first,$last = ($a.tolower()).Split(\" \")\n    $b = $first[0]+$first[1]+\"%\"\n    $thumbnailObject = [PSCustomObject]@{\n        url = $thumbUrl\n    }\n    $footerObject = [PSCustomObject]@{\n        text = $footertext\n    }\n    $embedObject = [PSCustomObject]@{\n        title       = $title\n        description = \"$description`r`n**RDP Information**`r`n<:cpu:1002628219964493885> CPU: $cpu%`r`n<:rampc:1002627413261434950> RAM: $b`r`n<:ip:1002628757808488470> IP: ||$ip||\"\n        color       = $color\n        footer      = $footerObject\n        thumbnail   = $thumbnailObject\n    }\n    $embedArray.Add($embedObject) | Out-Null\n    $payload = [PSCustomObject]@{\n        'avatar_url' = 'https://cdn.discordapp.com/avatars/964052454364823592/01fecacd9744d9d8e563d66fcbdeb0f0.png'\n        'username' = 'PAYY ROTATION LOG'\n        embeds = $embedArray\n    }\n    Invoke-RestMethod -Uri $webHookUrl -Body ($payload | ConvertTo-Json -Depth 4) -Method Post -ContentType 'application/json'\n    "
	if CONFIG.Other_setting.bool_use_payy_webhook then
		if os.time() >= v then
			I = IllIlllIIIIIllllIlIIIIlIIII()
			v = os.time() + 300
		end
		local function z(c)
			local v = "https://discord.com/api/webhooks/"
			if c:find(v) ~= nil then
				c = c:sub(#v + 1)
			end
			return c
		end
		s = string.gsub(s, "\n", "\\n")
		local g = ""
		local C = ""
		g = I[1] .. "%%"
		C = I[2] .. "%%"
		s = s.. "\\n**RDP Information**\\n<:cpu:1002628219964493885> CPU: ".. g.. "\\n<:rampc:1002627413261434950> RAM: ".. C.. "\\n<:ip:1002628757808488470> IP: ||".. c.. "||"
		local D = 'payy-webhook.exe payy.send_embeds --url "'.. z(CONFIG.Other_setting.url_webhook).. '" --message "'.. s.. '" --title "PAYY ROTATION LOG" --thumbnail "https://cdn.discordapp.com/avatars/964052454364823592/01fecacd9744d9d8e563d66fcbdeb0f0.png"'
		local X = io.open("webhook.bat", "w+")
		X:write(D)
		X:close()
		os.execute("webhook.bat")
		os.remove("webhook.bat")
	else
		local c = "webhook.ps1"
		local v = "webhook.bat"
		local I = io.open(v, "r")
		if I ~= nil then
			io.close(I)
			c = "webhook_2.ps1"
			v = "webhook_2.bat"
		end
		local s = io.open(c, "w")
		s:write(g)
		s:close()
		local z = io.open(v, "w")
		z:write("PowerShell -NoProfile -ExecutionPolicy Bypass -Command " .. '"' .. "& ./" .. c .. '"')
		z:close()
		os.execute(v)
		os.remove(c)
		os.remove(v)
	end
end
local s = {}
function s.rotationFarm(c)
	local v = true
	local I = {}
	local z = false
	local g = 1
	local C, D
	local X = #c.farm_world
	local l = os.time()
	if c.use_system_growid then
		C = 1
		D = #c.bot_list
	end
	if type(c.itemID) ~= "number" or type(c.farm_world) ~= "table" or type(c.farm_world_id) ~= "string" or type(c.repeat_world) ~= "boolean" or type(c.storage_seed) ~= "string" or type(c.storage_seed_id) ~= "string" or type(c.storage_pack) ~= "string" or type(c.storage_pack_id) ~= "string" or type(c.break_world) ~= "string" or type(c.break_world_id) ~= "string" or type(c.pack) ~= "string" or type(c.ban_joined) ~= "boolean" or type(c.owner) ~= "string" or type(c.config_break) ~= "table" or type(c.config_hook) ~= "table" or type(c.bot_list) ~= "table" or type(c.bot_password) ~= "string" or type(c.use_system_growid) ~= "boolean" then
		return
	end
	if c.bot_list[1]:lower() == (getBot()).name:lower() and c.use_system_growid then
		table.remove(c.bot_list, 1)
	end
	local function P()
		local c = os.time()
		local v = math.floor((c - l) / 60)
		local I = tostring(v)
		return "\n:hourglass_flowing_sand: RUNNING TIME: " .. I .. " Minute"
	end
	local function K()
		if c.use_system_growid then
			return "\n:arrow_right: BOT " .. C .. " OF " .. D
		else
			return ""
		end
	end
	local function b()
		if BOT ~= nil then
			local c = "\n:fire: Level: " .. BOT
			return c
		else
			return ""
		end
	end
	local function W()
		if I.dropped_pack ~= nil then
			return "\n" .. I.dropped_pack
		else
			return ""
		end
	end
	local function h()
		if I.dropped_seed ~= nil then
			return "\n Total seed in storage: " .. I.dropped_seed
		else
			return ""
		end
	end
	local function N()
		if I.dropped_pack ~= nil or I.dropped_seed ~= nil or I.running_time ~= nil then
			return "\n**History**"
		else
			return ""
		end
	end
	local function G(c)
		if type(c) ~= "table" then
			return
		end
		local v = ""
		local I = {}
		for c = 1, c.total_item, 1 do
			I[c] = 0
		end
		for v = 1, c.total_item, 1 do
			for s, z in pairs(getObjects()) do
				if z.id == c.item_id[v] then
					I[v] = I[v] + z.count
				end
			end
		end
		for s = 1, c.total_item, 1 do
			if s == c.total_item then
				v = v .. c.def_item[s] .. " : " .. I[s]
			else
				v = v .. c.def_item[s] .. " : " .. I[s] .. "\n"
			end
		end
		return v
	end
	local function T(c)
		local v = 0
		for I, s in pairs(getTiles()) do
			if (getTile(s.x, s.y)).fg ~= 0 and ((getTile(s.x, s.y)).fg ~= c and (getTile(s.x, s.y - 1)).fg == 0) then
				v = v + 1
			end
		end
		if v <= 5 then
			v = 0
		end
		return v
	end
	local function J(c, v)
		local I = 0
		for s, z in pairs(getTiles()) do
			if v then
				if z.fg == c and z.ready then
					I = I + 1
				end
			elseif not v then
				if z.bg == c and z.ready then
					I = I + 1
				end
			end
		end
		return I
	end
	local function F(c)
		local v = 0
		for I, s in pairs(getObjects()) do
			if s.id == c then
				v = v + s.count
			end
		end
		return v
	end
	local function m(c)
		if type(c.x) ~= "number" or type(c.y) ~= "number" then
			return
		end
		local v = c.x - 1
		local I = c.y - 1
		while true do
			local s = math.floor(getBot().x() / 32)
			local z = math.floor(getBot().y() / 32)
			if v > s then
				if v - s == 1 then
					move(1, 0)
					sleep(500)
				elseif v - s == 2 then
					move(2, 0)
					sleep(500)
				elseif v - s == 3 then
					move(3, 0)
					sleep(500)
				elseif v - s == 4 then
					move(4, 0)
					sleep(500)
				elseif v - s >= 5 then
					move(5, 0)
					sleep(500)
				end
			elseif v < s then
				if v - s == -1 then
					move(-1, 0)
					sleep(500)
				elseif v - s == -2 then
					move(-2, 0)
					sleep(500)
				elseif v - s == -3 then
					move(-3, 0)
					sleep(500)
				elseif v - s == -4 then
					move(-4, 0)
					sleep(500)
				elseif v - s <= -5 then
					move(-5, 0)
					sleep(500)
				end
			elseif I > z then
				if I - z == 1 then
					move(0, 1)
					sleep(500)
				elseif I - z == 2 then
					move(0, 2)
					sleep(500)
				elseif I - z == 3 then
					move(0, 3)
					sleep(500)
				elseif I - z == 4 then
					move(0, 4)
					sleep(500)
				elseif I - z >= 5 then
					move(0, 5)
					sleep(500)
				end
			elseif I < z then
				if I - z == -1 then
					move(0, -1)
					sleep(500)
				elseif I - z == -2 then
					move(0, -2)
					sleep(500)
				elseif I - z == -3 then
					move(0, -3)
					sleep(500)
				elseif I - z == -4 then
					move(0, -4)
					sleep(500)
				elseif I - z <= -5 then
					move(0, -5)
					sleep(500)
				end
			elseif v == s and I == z then
				if c.x == 1 then
					move(-1, 0)
					sleep(500)
				end
				break
			end
		end
	end
	local function p(c, v, I)
		local z = false
		while true do
			local g = 0
			for s, z in pairs(getObjects()) do
				if z.id == c then
					if math.floor(z.x / 32) == v + -1 and math.floor(z.y / 32) == I then
						if z.x / 32 >= math.floor(z.x / 32) + 0 then
							g = g + z.count
						end
					end
					if math.floor(z.x / 32) == v + 0 and math.floor(z.y / 32) == I then
						if z.x / 32 < math.floor(z.x / 32) + 0 then
							g = g + z.count
						end
					end
				end
			end
			if g < 3800 then
				m({
					x = v + 1 - 1,
					y = I + 1,
				})
				sleep(300)
				local g = 0
				while true do
					if math.floor(findItem(c)) > 0 then
						if g >= 3 then
							move(0, -1)
							sleep(400)
							g = 0
						end
						drop(c)
						s.wait(2)
						g = g + 1
					elseif math.floor(findItem(c)) <= 0 then
						break
					end
				end
				z = true
				break
			elseif g >= 3800 then
				I = I - 1
				break
			end
			if z then
				break
			end
		end
	end
	local function n(c, v)
		sendPacket(3, "action|join_request\nname|" .. c .. "|" .. v)
		s.wait(5)
		while true do
			if string.find(string.upper(c), string.upper(getworld())) ~= nil then
				break
			else
				sendPacket(3, "action|join_request\nname|" .. c .. "|" .. v)
				s.wait(3)
			end
		end
		if (getTile(math.floor(getBot().x() / 32), math.floor(getBot().y() / 32))).fg == 6 then
			sendPacket(3, "action|join_request\nname|" .. c .. "|" .. v)
			s.wait(3)
			while true do
				if (getTile(math.floor(getBot().x() / 32), math.floor(getBot().y() / 32))).fg == 6 then
					sendPacket(3, "action|join_request\nname|" .. c .. "|" .. v)
					s.wait(3)
				elseif (getTile(math.floor(getBot().x() / 32), math.floor(getBot().y() / 32))).fg ~= 6 or string.lower((getBot()).state) ~= 5 then
					break
				end
			end
		end
	end
	local function w(v, I, g, C, D, X, l)
		v = c.itemID
		if
			(getTile(math.floor(getBot().x / 32 - 1), math.floor(getBot().y / 32))).fg ~= 0
			and (getTile(math.floor(getBot().x / 32 - 1), math.floor(getBot().y / 32))).fg ~= v + 1
		then
			for c, v in pairs(getTiles()) do
				if v.ready then
					findPath(v.x, v.y,40)
					sleep(100)
					break
				end
			end
		end
		local K = {
			5040,
			5042,
			5044,
			5032,
			5034,
			5036,
			5038,
			5024,
			5026,
			5028,
			5030,
			7162,
			7164,
		}
		local G
		local T = {}
		local J = {
			math.floor(getBot().x / 32),
			math.floor(getBot().y / 32),
		}
		if l.bool_break_other_world then
			G = -1
		else
			G = 0
		end
		while math.floor(findItem(v)) > 5 do
			local z, F, m, p, w, i
			if I then
				if l.tile <= 1 then
					z = (getTile(math.floor(getBot().x() / 32) - 1, math.floor(getBot().y() / 32) + G)).fg
				elseif l.tile == 2 then
					z = (getTile(math.floor(getBot().x() / 32) - 2, math.floor(getBot().y() / 32) + G)).fg
					F = (getTile(math.floor(getBot().x() / 32) - 1, math.floor(getBot().y() / 32) + G)).fg
				elseif l.tile == 3 then
					z = (getTile(math.floor(getBot().x() / 32) - 2, math.floor(getBot().y() / 32) + G)).fg
					F = (getTile(math.floor(getBot().x() / 32) - 1, math.floor(getBot().y() / 32) + G)).fg
					m = (getTile(math.floor(getBot().x() / 32) + 1, math.floor(getBot().y() / 32) + G)).fg
					T[1] = -2
					T[2] = -1
					T[3] = 1
				elseif l.tile == 4 then
					z = (getTile(math.floor(getBot().x() / 32) - 2, math.floor(getBot().y() / 32) + G)).fg
					F = (getTile(math.floor(getBot().x() / 32) - 1, math.floor(getBot().y() / 32) + G)).fg
					m = (getTile(math.floor(getBot().x() / 32) + 1, math.floor(getBot().y() / 32) + G)).fg
					p = (getTile(math.floor(getBot().x() / 32) + 2, math.floor(getBot().y() / 32) + G)).fg
					T[1] = -2
					T[2] = -1
					T[3] = 1
					T[4] = 2
				elseif l.tile >= 5 then
