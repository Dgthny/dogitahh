						break
					elseif v.fg == 6 and v.y ~= math.floor(getBot().y() / 32) then
						break
					end
				end
				local v = math.floor(getBot().x() / 32)
				if v < 5 then
					move(5, 0)
					sleep(300)
				elseif v > 95 then
					move(-5, 0)
					sleep(300)
				end
